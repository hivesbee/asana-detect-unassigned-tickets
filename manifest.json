{
  "manifest_version": 3,
  "name": "asana-detect-unassigned-tickets",
  "version": "1.0.0",
  "description": "Asana の未アサインチケットを検出します",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": [
      "https://app.asana.com/0/1206158368532681/*",
      "https://app.asana.com/0/1206158365951310/*",
      "https://app.asana.com/0/1206946439584057/*"
    ],
    "css": ["css/style.css"] ,
    "js": ["scripts/content.js"]
  }]
}